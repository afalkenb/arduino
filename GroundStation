//GroundStation Code

const int StartButtonPin;
int first; //tells the Winch Arduino that the program has started

void setup() {

  first = 1; 
  StartButtonPin = 3;  //start button
  pinMode(StartButtonPin, INPUT)
  
  }

void loop() {

  if digitalRead(StartButtonPin == HIGH) {
    MasterCart(int first)
  }
}

