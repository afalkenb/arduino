//GroundStation Code

void setup() {

  const int StartButtonPin = 3;  //start button
  const int EmerButtonPin = 4;   //emergency button

  pinMode(StartButtonPin, INPUT)
  pinMode(EmerButtonPin, INPUT)
  }

void loop() {

  if digitalRead(StartButtonPin == HIGH) {
    begin()
  }
  
  if digitalRead(EmerButtonPIN == HIGH) {
  
    //NEED TO FIGURE OUT HOW TO EMERGENCY POWER OFF HERE
