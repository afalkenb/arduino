//GroundStation Code

void setup() {

  const int StartButtonPin = 3;  //start button
 

  pinMode(StartButtonPin, INPUT)
  
  }

void loop() {

  if digitalRead(StartButtonPin == HIGH) {
    begin()
  }
}

void begin() {

  //Tighten First
  tightenFunction()
  
  //Execute Code that climbs and controls
  MasterCart()
  
  //Loosen Here
  loosenFunction()
  
}


double tightenFunction{

}

double loosenFunction{

}
